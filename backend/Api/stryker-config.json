{
  "mutate": [
    "**/Core/**/*.cs"
  ],
  "testProjects": [
    "Test/UnitTests/UnitTests.csproj"
  ],
  "reporters": [
    "html",
    "progress"
  ],
  "logLevel": "info",
  "ignoreMutations": [
    "**/Core/Request/*.cs",
    "**/Core/UseCase/*Request.cs",
    "**/Core/UseCase/*Response.cs",
    "**/Api/Program.cs"
  ],
  "thresholds": {
    "high": 80,
    "low": 60,
    "break": 50
  },
  "concurrency": 4,
  "coverageAnalysis": "perTest"
}